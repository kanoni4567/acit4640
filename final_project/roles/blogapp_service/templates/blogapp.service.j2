[Unit]
Description=blog app, ACIT4640
After=network.target

[Service]
Environment=PORT={{  blogapp_port  }}
WorkingDirectory=/home/{{ blog_user }}/app
Type=simple
User={{ blog_user }}
ExecStart=/usr/bin/node /home/{{ blog_user }}/app/app.js
Restart=on-failure
RestartSec=10

[Install]
WantedBy=multi-user.target